<ehcache>
    <diskStore path="java.io.tmpdir"/>
    <cache  name="users"
            maxElementsInMemory="10000"
            eternal="false"
    timeToIdleSeconds="30"
    timeToLiveSeconds="30"
    overflowToDisk="true"
    />
    <!-- //如果为true 导致下面两个时间设置无效
    //闲置时间 （最后访问时间-当前时间）
    //存活时间（当前时间-创建时间）-->
</ehcache>